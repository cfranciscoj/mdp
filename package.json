{
  "name": "mdp",
  "version": "0.0.9",
  "license": "Apache-2.0",
  "repository": "https://github.com/ericlink/mdp",
  "productName": "mdp",
  "description": "mdp",
  "author": "elink",
  "main": "./src/main.js",
  "keywords": [
    "markdown",
    "electron",
    "livereload",
    "live-reload"
  ],
  "scripts": {
    "dev-publish": "npm run package && rm -rf /Applications/mdp.app/ && mv ./out/mdp-darwin-x64/mdp.app /Applications/",
    "dev": "./node_modules/.bin/electron ./src/main.js ./assets/example.md",
    "log": "tail -f ~/Library/Logs/mdp/log.log",
    "dev-noarg": "./node_modules/.bin/electron ./src/main.js",
    "dev-readme": "./node_modules/.bin/electron ./src/main.js ./README.md",
    "start": "electron-forge start ./assets/example.md",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "dependencies": {
    "chokidar": "^3.0.1",
    "cryptiles": "^4.1.3",
    "electron-cookies": "*",
    "electron-log": "^3.0.6",
    "electron-reload": "^1.4.0",
    "electron-settings": "^3.2.0",
    "electron-window-state": "^5.0.2",
    "highlight.js": "^9.15.8",
    "hoek": "^6.1.3",
    "marked": "^0.8.0",
    "mermaid": "^8.0.0",
    "node-emoji": "^1.10.0",
    "tmp": "^0.1.0",
    "update-electron-app": "^1.4.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.47",
    "@electron-forge/maker-deb": "^6.0.0-beta.47",
    "@electron-forge/maker-rpm": "^6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.47",
    "@electron-forge/maker-zip": "^6.0.0-beta.47",
    "@electron-forge/publisher-github": "^6.0.0-beta.47",
    "electron": "^5.0.13",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/app.icns",
        "darwinDarkModeSupport": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ],
          "config": {}
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "platforms": [
            "darwin"
          ],
          "config": {
            "repository": {
              "owner": "ericlink",
              "name": "mdp"
            },
            "prerelease": false
          }
        }
      ]
    }
  }
}
